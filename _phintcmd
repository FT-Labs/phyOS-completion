sudo blockdev --report:Print report for block devices
sudo blkid _dev_ -D  --match-tag UUID -o value:Get _dev_ UUID value
sudo dmsetup ls --tree --table devname -o blkdevname,compact:Print dm deps in each line
sudo dmsetup ls --tree --table devname -o blkdevname,compact | grep $(mount | awk '/on \/ / {n=split($1,a,"/"); print a[n]}') | awk '{print $(NF-1)}' | tr -d "<>":Get devname of encrypted root partition
sudo losetup -a:Print loop device status
sudo pacman -R --nodeps _pkg_
udevadm info -a -n _dev_ | egrep 'looking|DRIVER'
sudo debugfs -R "stat _file_" _dev_ | grep -o "Flags:.*"
ps -elFL
mount -n --bind -o ro /root/cmdline /proc/cmdline # Hijack cmdline
grep --text -m 1 "video mode" -A2 _kernel_ | tail -1 | awk '{print $1}'
pacman -Qlq _pkg_ | while read f; do [[ -d $f ]] && mkdir -p "./$f" || cp $f "./$f"; done # pac make pkg
sudo nmap -sn #Scan local network
sudo xxd -len _size_ -cols _rowlen_ /dev/nvme0n1 #print bytes
